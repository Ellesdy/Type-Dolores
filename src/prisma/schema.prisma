generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Member {
  id          Int       @id @default(autoincrement())
  discordId   String    @unique
  username    String
  guildName   String
  active      Boolean   @default(true)
  joinDate    DateTime  @default(now())
  rejoinDate  DateTime? @default(now())
  leaveDate   DateTime?
  karmaPoints Int       @default(0)
}
